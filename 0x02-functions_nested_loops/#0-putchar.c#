#include "main.h"
#include <stu
/**
 * _putchar - writes the character c to stdout
 * @c: The character to print
 *
 * Return: Always 0 (success)
 * On error, -1 is returned, and errno is set appropriately.
 */
int main(void)
{
int _putchar(char c);
_putchar c[] = {'_', 'p', 'u', 't', 'c', 'h', 'a', 'r'};
int n = 0;
while (n <=)
{
_putchar(c[n]);
n++;
}
_putchar('\n');
return (0);
}
