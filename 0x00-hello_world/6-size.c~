#include <stdio.h>
int main(void)
{
char acharacter;
int ainteger;
long along;
long long alonglong;
float afloat;
printf("Size of char: %zu byte\n", sizeof(acharacter));
printf("Size of int: %zu bytes\n", sizeof(ainteger));
printf("Size of long: %zu bytes\n", sizeof(along));
printf("Size of alonglong: %zu bytes\n", sizeof(alonglong));
printf("Size of float: %zu bytes\n", sizeof(afloat));
return (0); 
}
